[
    {
        "id": 42219,
        "title": "二贺诗",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "学行可师贺德基，文质彬彬贺德仁。"
        ]
    },
    {
        "id": 42220,
        "title": "四王语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "宁向儋崖振白，不事江滕蒋虢。"
        ]
    },
    {
        "id": 42221,
        "title": "时人为屈突语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "宁食三斗艾，不见屈突盖。宁服三斗葱，不逢屈突通。"
        ]
    },
    {
        "id": 42222,
        "title": "杨刺史语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "宁食三斗蒜，不逢杨德干。"
        ]
    },
    {
        "id": 42223,
        "title": "万年人语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "宁饮三斗尘，无逢权怀恩。"
        ]
    },
    {
        "id": 42224,
        "title": "赞皇人语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "太冲无兄，孝端无弟。"
        ]
    },
    {
        "id": 42225,
        "title": "高宗时语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "左相宣威沙漠，右相驰誉丹青。",
            "三馆学生放散，五台令史经明。"
        ]
    },
    {
        "id": 42226,
        "title": "时人为李义甫语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "今日巨唐年，还诛四凶族。"
        ]
    },
    {
        "id": 42227,
        "title": "洛州语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "洛州有前贾后张，可敌京兆三王。"
        ]
    },
    {
        "id": 42228,
        "title": "江淮间语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "贵如许郝，富若田彭。"
        ]
    },
    {
        "id": 42229,
        "title": "河北语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "唯此两何，杀人最多。"
        ]
    },
    {
        "id": 42230,
        "title": "李昭德为王弘义语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "昔闻苍鹰狱吏，今见白兔御史。"
        ]
    },
    {
        "id": 42231,
        "title": "京洛语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "衣裳好，仪貌恶。不姓许，即姓郝。"
        ]
    },
    {
        "id": 42232,
        "title": "台中语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "侯知一不伏致仕，张悰自请起复。高筠不肯作孝，"
        ]
    },
    {
        "id": 42233,
        "title": "吏人语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "不畏侯卿杖，惟畏尹卿笔。"
        ]
    },
    {
        "id": 42234,
        "title": "选人语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "有钱石上好，无钱刘下好，士大夫张下好。",
            "硕学师刘子，儒生用与言。",
            "今年柿子并遭霜，为语石榴须早摘。"
        ]
    },
    {
        "id": 42235,
        "title": "韦氏语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "大郎罢相，小郎拜相。"
        ]
    },
    {
        "id": 42236,
        "title": "益州人吏语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "录事意，与天通，益州司马折威风。"
        ]
    },
    {
        "id": 42237,
        "title": "天授中语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "遇徐杜者必生，遇来侯者必死。"
        ]
    },
    {
        "id": 42238,
        "title": "时人为邹昉语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "萧佺驸马子，邹昉骆驼儿。非关道德合，只为钱相知。"
        ]
    },
    {
        "id": 42239,
        "title": "题张昌仪门语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "一两丝能得几时络。"
        ]
    },
    {
        "id": 42240,
        "title": "时人号李知远语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "李下无蹊。"
        ]
    },
    {
        "id": 42241,
        "title": "又号李乂语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "李下无蹊径。"
        ]
    },
    {
        "id": 42242,
        "title": "窦仆射语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "窦仆射，前为韦氏国yQ，后作公主邑丞。"
        ]
    },
    {
        "id": 42243,
        "title": "时人为崔无诐语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "皇后嫁女，天子娶妇。"
        ]
    },
    {
        "id": 42244,
        "title": "神龙中语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "崔冉郑，乱时政。"
        ]
    },
    {
        "id": 42245,
        "title": "斜封官语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "姚宋为相，邪不如正。太平用事，正不如邪。"
        ]
    },
    {
        "id": 42246,
        "title": "李处郁语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "飧若入咽，百无一全。"
        ]
    },
    {
        "id": 42247,
        "title": "景云初语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "前有裴马，后有卢李。"
        ]
    },
    {
        "id": 42248,
        "title": "先天时京中语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "姜师度一心看地，傅孝忠两眼相天。"
        ]
    },
    {
        "id": 42249,
        "title": "时人号王丘、崔沔语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "丘山岌岌连天峻，沔水澄澄澈底清。"
        ]
    },
    {
        "id": 42250,
        "title": "吏部过官语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "麟之口，光庭手。"
        ]
    },
    {
        "id": 42251,
        "title": "郇公厨语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "人欲不饭筋骨舒，夤缘须入郇公厨。"
        ]
    },
    {
        "id": 42252,
        "title": "四俊语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "令君四俊，苗吕崔员。"
        ]
    },
    {
        "id": 42253,
        "title": "八友语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "殷颜柳陆，李萧邵赵。"
        ]
    },
    {
        "id": 42254,
        "title": "罗吉口号",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "罗钳吉网。"
        ]
    },
    {
        "id": 42255,
        "title": "里闾诅语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "若违教，值三豹。"
        ]
    },
    {
        "id": 42256,
        "title": "陕州语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "不须赛神明，不必求巫祝。尔莫犯卢公，立便有祸福。"
        ]
    },
    {
        "id": 42257,
        "title": "真源邑语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "南金口，明府手。"
        ]
    },
    {
        "id": 42258,
        "title": "称二王语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "朝廷左相笔，天下右丞诗。"
        ]
    },
    {
        "id": 42259,
        "title": "代宗朝京师语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "常无分别元好钱，贤者愚，愚者贤。"
        ]
    },
    {
        "id": 42260,
        "title": "戏谏司语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "韩谏议虽分左右，归拾遗莫辨存亡。"
        ]
    },
    {
        "id": 42261,
        "title": "裴度语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "鸡猪鱼蒜，逢著则吃。生老病死，时至则行。"
        ]
    },
    {
        "id": 42262,
        "title": "魏博语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "长安天子，魏府牙军。"
        ]
    },
    {
        "id": 42263,
        "title": "宝历宫中语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "宝帐香重重，一双红芙蓉。"
        ]
    },
    {
        "id": 42264,
        "title": "京师人号牛杨语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "太牢笔，少牢口，东西南北何处走。"
        ]
    },
    {
        "id": 42265,
        "title": "又号牛李",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "门生故吏，不牛则李。。"
        ]
    },
    {
        "id": 42266,
        "title": "荆南语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "旱不苦，祷而雨。雨不愁，公出游。"
        ]
    },
    {
        "id": 42267,
        "title": "郑仁表自语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "天瑞有五色云，人瑞有郑仁表。"
        ]
    },
    {
        "id": 42268,
        "title": "湖苏二郡语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "湖接两头，苏联三尾。"
        ]
    },
    {
        "id": 42269,
        "title": "广明初都人语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "黄贼打黑贼。"
        ]
    },
    {
        "id": 42270,
        "title": "吏部旧语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "长名以前，选人属侍郎。长名以后，侍郎属选人。"
        ]
    },
    {
        "id": 42271,
        "title": "省中语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "后行祠屯，不博中行都门；中行礼部，不博前行驾库。"
        ]
    },
    {
        "id": 42272,
        "title": "郎吏语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "司门水部，入省不数。"
        ]
    },
    {
        "id": 42273,
        "title": "谏院台省语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "遗补相惜，御史相憎，郎官相轻。"
        ]
    },
    {
        "id": 42274,
        "title": "御史台语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "事长如事端。"
        ]
    },
    {
        "id": 42275,
        "title": "京兆府语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "不立两县令，不坐两少尹。"
        ]
    },
    {
        "id": 42276,
        "title": "翰林谏议语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "饶道斗上坡去，亦须却下坡来。"
        ]
    },
    {
        "id": 42277,
        "title": "举子语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "槐花黄，举子忙。"
        ]
    },
    {
        "id": 42278,
        "title": "明经进士语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "三十老明经，五十少进士。"
        ]
    },
    {
        "id": 42279,
        "title": "杂帖语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "常杂鲍帖。"
        ]
    },
    {
        "id": 42280,
        "title": "闽人语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "欧阳独步，藻蕴横行。"
        ]
    },
    {
        "id": 42281,
        "title": "举场语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "欲入举场，先问苏张。苏张犹可，三杨杀我。"
        ]
    },
    {
        "id": 42282,
        "title": "大中后进士语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "欲得命通，问瑝嵎都雍。"
        ]
    },
    {
        "id": 42283,
        "title": "大中时语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "郑杨段薛，炙手可热。欲得命通，鲁绍瑰蒙。"
        ]
    },
    {
        "id": 42284,
        "title": "选举人语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "王凝裴瓒。舍弟安潜。朝中无呼字，知闻厅里，"
        ]
    },
    {
        "id": 42285,
        "title": "科目举人语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "未见王窦，徒劳漫走。"
        ]
    },
    {
        "id": 42286,
        "title": "戏杜审权语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "座主审权，门生处权。"
        ]
    },
    {
        "id": 42287,
        "title": "崔沆放榜时人语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "座主门生，沆瀣一家。"
        ]
    },
    {
        "id": 42288,
        "title": "唐末五代人语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "及第不必读书，作官何须事业。"
        ]
    },
    {
        "id": 42289,
        "title": "号沈宋语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "苏李居前，沈宋比肩。"
        ]
    },
    {
        "id": 42290,
        "title": "号钱郎语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "前有沈宋，后有钱郎。"
        ]
    },
    {
        "id": 42291,
        "title": "潘何诗赋语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "潘纬十年吟古镜，何涓一夜赋潇湘。"
        ]
    },
    {
        "id": 42292,
        "title": "魏薛草书语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "前有虞褚，后有薛魏。"
        ]
    },
    {
        "id": 42293,
        "title": "薛稷书语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "买褚得薛不落节。"
        ]
    },
    {
        "id": 42294,
        "title": "时人为刘毕语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "刘郎中松树孤标，毕庶子松根绝妙。"
        ]
    },
    {
        "id": 42295,
        "title": "时人为黄筌语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "黄筌画鹤，薛稷减价。"
        ]
    },
    {
        "id": 42296,
        "title": "时人为杨惠之语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "道子画，惠之塑，夺得僧繇神笔路。"
        ]
    },
    {
        "id": 42297,
        "title": "道杰语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "大头杰，难杀人。"
        ]
    },
    {
        "id": 42298,
        "title": "马周疏引俚语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "贫不学俭，富不学奢。"
        ]
    },
    {
        "id": 42299,
        "title": "杜甫引俚语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "城南韦杜，去天尺五。"
        ]
    },
    {
        "id": 42300,
        "title": "《孙光宪琐言》引古语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "乘船走马，去死一分。",
            "好事不出门，恶事行千里。"
        ]
    },
    {
        "id": 42301,
        "title": "杜重威引俚语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "逢贼得命，更望复子。"
        ]
    },
    {
        "id": 42302,
        "title": "建安语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "龙门一半在闽川。"
        ]
    },
    {
        "id": 42303,
        "title": "江陵语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "琵琶多于饭甑，措大多于鲫鱼。"
        ]
    },
    {
        "id": 42304,
        "title": "汾晋间语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "欲作千箱主，问取黄金母。"
        ]
    },
    {
        "id": 42305,
        "title": "秦中儿童语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "颠当牢守门，蠮螉寇汝无处奔。"
        ]
    },
    {
        "id": 42306,
        "title": "沧洲语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "不戴金莲花，不得到仙家。"
        ]
    },
    {
        "id": 42307,
        "title": "《段成式酉阳杂俎》引古语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "三守庚申三尸伏，七守庚申七尺灭。"
        ]
    },
    {
        "id": 42308,
        "title": "佛书引语",
        "author": "不详",
        "volume": "卷八百七十六",
        "paragraphs": [
            "停囚长智。",
            "赤脚人趁兔，著靴人吃肉。"
        ]
    }
