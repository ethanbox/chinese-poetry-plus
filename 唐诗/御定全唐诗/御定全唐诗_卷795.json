[
    {
        "id": 38437,
        "title": "句",
        "author": "李日知",
        "paragraphs": [
            "所愿暂知居者乐，无使时称主者劳。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38438,
        "title": "句",
        "author": "赵仁奖",
        "paragraphs": [
            "令乘骢马去，丞脱绣衣来。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38439,
        "title": "句",
        "author": "郭廷谓",
        "paragraphs": [
            "魂逐东流水，坟依独坐山。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38440,
        "title": "句",
        "author": "韦青",
        "paragraphs": [
            "三代掌纶诰，一身能唱歌。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38441,
        "title": "句",
        "author": "尉迟匡",
        "paragraphs": [
            "明月飞出海，黄河流上天。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38442,
        "title": "句",
        "author": "尉迟匡",
        "paragraphs": [
            "芙蓉初出水，桃李忽无言。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38443,
        "title": "句",
        "author": "尉迟匡",
        "paragraphs": [
            "夜夜月为青冢镜，年年雪作黑山花。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38444,
        "title": "句",
        "author": "何涓",
        "paragraphs": [
            "雁影数行秋半逢，渔歌一声夜深发。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38445,
        "title": "句",
        "author": "杜伟",
        "paragraphs": [
            "忽睹邢武辞，泠其金石备。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38446,
        "title": "句",
        "author": "申堂构",
        "paragraphs": [
            "霜添柏树冷，气拂桂林寒。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38447,
        "title": "句",
        "author": "周愿",
        "paragraphs": [
            "八十年前棠树阴，竟陵太守公先人。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38448,
        "title": "句",
        "author": "苐五琦",
        "paragraphs": [
            "阴天闻断雁，夜浦送归人。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38449,
        "title": "句",
        "author": "颜允南",
        "paragraphs": [
            "谁言百人会，兄弟也沾陪。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38450,
        "title": "句",
        "author": "季广琛",
        "paragraphs": [
            "刻舟寻已化，弹铗未酬恩。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38451,
        "title": "句",
        "author": "陈蜕",
        "paragraphs": [
            "梦里换春秋。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38452,
        "title": "句",
        "author": "卫准",
        "paragraphs": [
            "莫言闲话是闲话，往往事从闲话来。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38453,
        "title": "句",
        "author": "卫准",
        "paragraphs": [
            "何必剃头为弟子，无家便是出家人。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38454,
        "title": "句",
        "author": "杜鸿渐",
        "paragraphs": [
            "常愿追禅理，安能挹化源。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38455,
        "title": "句",
        "author": "李挚",
        "paragraphs": [
            "因缘三纪异，契分四般同。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38456,
        "title": "句",
        "author": "韦绶",
        "paragraphs": [
            "在室愧屋漏。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38457,
        "title": "句",
        "author": "冯戡",
        "paragraphs": [
            "桃花浪里成龙去，竹叶山头退鹢飞。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38458,
        "title": "句",
        "author": "韩泰",
        "paragraphs": [
            "庾岭东边吏隐州，溪山竹树亦清幽。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38459,
        "title": "句",
        "author": "卢并",
        "paragraphs": [
            "地灵无俗草，岩静有仙禽。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38460,
        "title": "句",
        "author": "缪岛云",
        "paragraphs": [
            "白鸟远行树，玉虹孤饮潭。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38461,
        "title": "句",
        "author": "缪岛云",
        "paragraphs": [
            "四五片霞生绝壁，两三行雁过疏松。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38462,
        "title": "句",
        "author": "缪岛云",
        "paragraphs": [
            "抛芥子降颠狒狒，折杨枝洒醉猩猩。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38463,
        "title": "句",
        "author": "刘敬之",
        "paragraphs": [
            "山近衡阳虽少雁，水连巴蜀岂无鱼。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38464,
        "title": "句",
        "author": "李涛",
        "paragraphs": [
            "水声长在耳，山色不离门。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38465,
        "title": "句",
        "author": "李涛",
        "paragraphs": [
            "扫地树留影，拂床琴有声。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38466,
        "title": "句",
        "author": "李涛",
        "paragraphs": [
            "落日长安道，秋槐满地花。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38467,
        "title": "句",
        "author": "高元裕",
        "paragraphs": [
            "中丞为国拔贤才，寒俊欣逢藻鉴开。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38468,
        "title": "句",
        "author": "韦澳",
        "paragraphs": [
            "莫将韦监同殷监，错认容身是保身。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38469,
        "title": "句",
        "author": "王龟",
        "paragraphs": [
            "珠箔卷繁星，金樽泻月明。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38470,
        "title": "句",
        "author": "李郁",
        "paragraphs": [
            "身死为修刘稹表，名高因让陆洿书。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38471,
        "title": "句",
        "author": "詹雄",
        "paragraphs": [
            "尘飞遗恨尽，花落古宫平。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38472,
        "title": "句",
        "author": "詹雄",
        "paragraphs": [
            "红粉笙歌人代远，月明陵树水东流。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38473,
        "title": "句",
        "author": "任涛",
        "paragraphs": [
            "露团沙鹤起，人卧钓船流。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38474,
        "title": "句",
        "author": "剧燕",
        "paragraphs": [
            "只向国门安四海，不离乡井拜三公。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38475,
        "title": "句",
        "author": "王璘",
        "paragraphs": [
            "芍药花开菩萨面，棕闾叶散野人头。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38476,
        "title": "句",
        "author": "李蔚",
        "paragraphs": [
            "虚心纤质雁衔余，凤吹龙吟定不如。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38477,
        "title": "句",
        "author": "王枳",
        "paragraphs": [
            "今朝拜贡盈襟泪，不进新芽是进心。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38478,
        "title": "句",
        "author": "郑賨",
        "paragraphs": [
            "万蕴千牌次碧牙，缥笺金字间明霞。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38479,
        "title": "句",
        "author": "张茔",
        "paragraphs": [
            "一箭不中鹄，五湖归钓鱼。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38480,
        "title": "句",
        "author": "吴霭",
        "paragraphs": [
            "烟随红焰断，化作白云飞。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38481,
        "title": "句",
        "author": "陈咏",
        "paragraphs": [
            "隔岸水牛浮鼻渡，傍溪沙鸟点头行。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38482,
        "title": "句",
        "author": "蒋密",
        "paragraphs": [
            "绮罗因片叶，桃李谩同时。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38483,
        "title": "句",
        "author": "符蒙",
        "paragraphs": [
            "都缘心似水，故以钵为舟。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38484,
        "title": "句",
        "author": "李景遂",
        "paragraphs": [
            "路指丹阳分虎节，心存双阙恋龙颜。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38485,
        "title": "句",
        "author": "李弘茂",
        "paragraphs": [
            "甜于泉水茶须信，狂似杨花蝶未知。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38486,
        "title": "句",
        "author": "李弘茂",
        "paragraphs": [
            "半窗月在犹煎药，几夜灯闲不照书。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38487,
        "title": "句",
        "author": "李平",
        "paragraphs": [
            "至人无梦梦不到，天道恶盈盈有余。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38488,
        "title": "句",
        "author": "李平",
        "paragraphs": [
            "龙髯已断嫔嫱老，豹尾不来岐路长。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38489,
        "title": "句",
        "author": "胡元龟",
        "paragraphs": [
            "翻身腾白浪，探爪擢明珠。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38490,
        "title": "句",
        "author": "蒋钩",
        "paragraphs": [
            "因借梦书过竹寺，学耕秋粟绕茆原。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38491,
        "title": "句",
        "author": "蒋钩",
        "paragraphs": [
            "芭蕉叶上无愁雨，自是多情听断肠。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38492,
        "title": "句",
        "author": "史虚白",
        "paragraphs": [
            "风雨揭却屋，全家醉不知。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38493,
        "title": "句",
        "author": "夏宝松",
        "paragraphs": [
            "孤猿叫落中岩月，野客吟残半夜灯。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38494,
        "title": "句",
        "author": "夏宝松",
        "paragraphs": [
            "雁飞南浦砧初断，月满西楼酒半醒。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38495,
        "title": "句",
        "author": "夏宝松",
        "paragraphs": [
            "晓来羸驷依前去，目断遥山数点清。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38496,
        "title": "句",
        "author": "赵庆",
        "paragraphs": [
            "迈古文章金𬸚𬸦，出群行止玉麒麟。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38497,
        "title": "句",
        "author": "潘天锡",
        "paragraphs": [
            "风便磬声远，日斜楼影长。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38498,
        "title": "句",
        "author": "朱勰",
        "paragraphs": [
            "好是晚来香雨里，担簦亲送绮罗人。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38499,
        "title": "句",
        "author": "齐镐",
        "paragraphs": [
            "莲中花更好，云里月常新。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38500,
        "title": "句",
        "author": "黄可",
        "paragraphs": [
            "天下传将舞马赋，门前迎得跨驴宾。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38501,
        "title": "句",
        "author": "高元矩",
        "paragraphs": [
            "砚贮寒泉碧，庭堆败叶红。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38502,
        "title": "句",
        "author": "高元矩",
        "paragraphs": [
            "燕掠琴弦穿静院，吏收诗草下闲庭。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38503,
        "title": "句",
        "author": "陈贶",
        "paragraphs": [
            "年年闻尔者，未有不伤情。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38504,
        "title": "句",
        "author": "陈贶",
        "paragraphs": [
            "出得风尘者，合知岐路人。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38505,
        "title": "句",
        "author": "陈贶",
        "paragraphs": [
            "拂榻灯未来，开门月先入。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38506,
        "title": "句",
        "author": "陈贶",
        "paragraphs": [
            "忽生云是匣，高以月为台。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38507,
        "title": "句",
        "author": "陈贶",
        "paragraphs": [
            "入夜虽无伤物意，向明还有动人心。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38508,
        "title": "句",
        "author": "赵休",
        "paragraphs": [
            "金茎来白露，玉宇起清风。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38509,
        "title": "句",
        "author": "庸仁杰",
        "paragraphs": [
            "红旆渡江霞蘸水，青蛇出匣雪侵衣。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38510,
        "title": "句",
        "author": "庸仁杰",
        "paragraphs": [
            "云散便凝千里望，日斜长占半城阴。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38511,
        "title": "句",
        "author": "庸仁杰",
        "paragraphs": [
            "只住此山宁有意，向来求佛本无心。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38512,
        "title": "句",
        "author": "邵拙",
        "paragraphs": [
            "万国不得雨，孤云犹在山。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38513,
        "title": "句",
        "author": "毛炳",
        "paragraphs": [
            "先生不在此，千载只空山。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38514,
        "title": "句",
        "author": "陈德诚",
        "paragraphs": [
            "建水旧传刘夜坐，螺川新有夏江城。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38515,
        "title": "句",
        "author": "陈甫",
        "paragraphs": [
            "一雨洗残暑，万家生早凉。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38516,
        "title": "句",
        "author": "陈甫",
        "paragraphs": [
            "暮鸟归巢急，寒牛下陇迟。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38517,
        "title": "句",
        "author": "陈甫",
        "paragraphs": [
            "算吟千百首，方得两三联。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38518,
        "title": "句",
        "author": "陈甫",
        "paragraphs": [
            "清时不作登龙客，绿鬓闲梳傍草堂。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38519,
        "title": "句",
        "author": "徐融",
        "paragraphs": [
            "淮船分蚁队，江市聚蝇声。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38520,
        "title": "句",
        "author": "高若拙",
        "paragraphs": [
            "人间不见月，天外自分明。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38521,
        "title": "句",
        "author": "皮光业",
        "paragraphs": [
            "烧平樵路出，潮落海山高。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38522,
        "title": "句",
        "author": "皮光业",
        "paragraphs": [
            "行人折柳和轻絮，飞燕衔泥带落花。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38523,
        "title": "句",
        "author": "屠瓌智",
        "paragraphs": [
            "轻身都是义，殉主始为忠。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38524,
        "title": "句",
        "author": "元德昭",
        "paragraphs": [
            "满堂罗绮兼朱紫，四代儿孙奉老翁。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38525,
        "title": "句",
        "author": "林无隐",
        "paragraphs": [
            "雪消二月江湖阔，花发千山道路香。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38526,
        "title": "句",
        "author": "杨义方",
        "paragraphs": [
            "海边红日半离水，天外暖风轻到花。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38527,
        "title": "句",
        "author": "杨义方",
        "paragraphs": [
            "雨声鞭自禁门出，一簇人从天上来。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38528,
        "title": "句",
        "author": "王廷珪",
        "paragraphs": [
            "十字水中分岛屿，数重花外见楼台。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38529,
        "title": "句",
        "author": "欧阳彬",
        "paragraphs": [
            "无钱将乞樊知客，名纸生毛不为通。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38530,
        "title": "句",
        "author": "欧阳彬",
        "paragraphs": [
            "桑柘残阳里，儿孙落叶中。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38531,
        "title": "句",
        "author": "李尧夫",
        "paragraphs": [
            "向外疑无地，其中别有天。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38532,
        "title": "句",
        "author": "李尧夫",
        "paragraphs": [
            "方外共推为道友，关中独自占诗家。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38533,
        "title": "句",
        "author": "李尧夫",
        "paragraphs": [
            "炎暑郁蒸无处避，凉风消息几时来。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38534,
        "title": "句",
        "author": "石文德",
        "paragraphs": [
            "月沈湘浦冷，花谢汉宫秋。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38535,
        "title": "句",
        "author": "戴偃",
        "paragraphs": [
            "须把咽喉吞世界，盖因奢侈致危亡。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38536,
        "title": "句",
        "author": "戴偃",
        "paragraphs": [
            "若须抛却便抛却，莫待风高更亦深。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38537,
        "title": "句",
        "author": "张迥",
        "paragraphs": [
            "夜长灯影灭，天绕鹤声孤。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38538,
        "title": "句",
        "author": "钟元章",
        "paragraphs": [
            "金筈离弦三尺电，星髇破的一声雷。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38539,
        "title": "句",
        "author": "杨凫",
        "paragraphs": [
            "背日流泉生冻早，逆风归鸟入巢迟。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38540,
        "title": "句",
        "author": "王俊",
        "paragraphs": [
            "胜日登楼望，山川一半春。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38541,
        "title": "句",
        "author": "薛沆",
        "paragraphs": [
            "也知别有风光主，花蕾枝枝似去年。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38542,
        "title": "句",
        "author": "张𫖮",
        "paragraphs": [
            "金殿圣人看纵笔，玉堂词客尽裁诗。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38543,
        "title": "句",
        "author": "张林",
        "paragraphs": [
            "菱叶乍翻人采后，芰荷初没舸行时。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38544,
        "title": "句",
        "author": "卢休",
        "paragraphs": [
            "春寒酒力迟，冉冉生微红。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38545,
        "title": "句",
        "author": "卢休",
        "paragraphs": [
            "自然草木性，谁祝元化功。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38546,
        "title": "句",
        "author": "卢休",
        "paragraphs": [
            "湓浦风生破胆愁。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38547,
        "title": "句",
        "author": "卢休",
        "paragraphs": [
            "血染剑花明帐幕，三千车马出渔阳。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38548,
        "title": "句",
        "author": "卢休",
        "paragraphs": [
            "入门堪笑复堪怜，三径苔荒一钓船。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38549,
        "title": "句",
        "author": "陈峤",
        "paragraphs": [
            "小桥风月年年事，争奈潘郎去□何。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38550,
        "title": "句",
        "author": "李范",
        "paragraphs": [
            "清猿啼远木，白鸟下前滩。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38551,
        "title": "句",
        "author": "李范",
        "paragraphs": [
            "鹤归秋汉远，人去草堂空。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38552,
        "title": "句",
        "author": "李范",
        "paragraphs": [
            "虽当南北路，不碍往来人。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38553,
        "title": "句",
        "author": "李范",
        "paragraphs": [
            "钓叟无机沙鸟睡，禅师入定白牛闲。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38554,
        "title": "句",
        "author": "李范",
        "paragraphs": [
            "天涯故友无来信，窗外拒霜空落花。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38555,
        "title": "句",
        "author": "卞震",
        "paragraphs": [
            "雨壁长秋菌，风枝落病蝉。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38556,
        "title": "句",
        "author": "卞震",
        "paragraphs": [
            "老笻搘瘦影，寒木凭吟身。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38557,
        "title": "句",
        "author": "卞震",
        "paragraphs": [
            "诗债到春无处避，离愁因醉暂时无。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38558,
        "title": "句",
        "author": "卞震",
        "paragraphs": [
            "茶香解睡磨铛煮，山色牵怀著屐登。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38559,
        "title": "句",
        "author": "林楚才",
        "paragraphs": [
            "身闲不恨辞官早，诗好常甘得句迟。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38560,
        "title": "句",
        "author": "孟不疑",
        "paragraphs": [
            "白日故乡远，青山佳句中。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38561,
        "title": "句",
        "author": "厐季子",
        "paragraphs": [
            "冥云生易满，秋草长难高。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38562,
        "title": "句",
        "author": "郭思",
        "paragraphs": [
            "落星一石几千年，门外何人扣汉川。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38563,
        "title": "句",
        "author": "卢载",
        "paragraphs": [
            "五千里地望皆见，七十二峰中最高。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38564,
        "title": "句",
        "author": "郑翱",
        "paragraphs": [
            "花烧落第眼，雨破到家程。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38565,
        "title": "句",
        "author": "郑说",
        "paragraphs": [
            "架上紫衣闲不著，案头金字坐长看。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38566,
        "title": "句",
        "author": "史瑜",
        "paragraphs": [
            "溪从沮水流嶓冢，岭接青泥入剑天。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38567,
        "title": "句",
        "author": "贺公",
        "paragraphs": [
            "但存方寸地，留与子孙耕。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38568,
        "title": "句",
        "author": "米都知",
        "paragraphs": [
            "小旗村店酒，微雨野塘花。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38569,
        "title": "句",
        "author": "陈秀才",
        "paragraphs": [
            "地偏云自去，日暮山更深。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38570,
        "title": "句",
        "author": "崔李二生",
        "paragraphs": [
            "恰似传花人饮散，空抛床下最繁枝。--崔",
            "艳魄香魂如有在，还应羞见堕楼人。--李"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38571,
        "title": "句",
        "author": "范氏子",
        "paragraphs": [
            "扫叶随风便，浇花趁日阴。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38572,
        "title": "句",
        "author": "范氏子",
        "paragraphs": [
            "闲云生不雨，病叶落非秋。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38573,
        "title": "句",
        "author": "临川小吏",
        "paragraphs": [
            "三月能辞千日醉，一生能得几回看。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38574,
        "title": "句",
        "author": "韩熙载客",
        "paragraphs": [
            "最是五更留不住，向人枕畔著衣裳。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38575,
        "title": "句",
        "author": "段义宗",
        "paragraphs": [
            "悬心秋夜月，万里照乡关。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38576,
        "title": "句",
        "author": "段义宗",
        "paragraphs": [
            "玉排复道珊瑚殿，金错危椆翡翠楼。"
        ],
        "volume": "卷七百九十五"
    },
    {
        "id": 38577,
        "title": "句",
        "author": "段义宗",
        "paragraphs": [
            "此花不与众花同，为感高僧护法功。"
        ],
        "volume": "卷七百九十五"
    }
]