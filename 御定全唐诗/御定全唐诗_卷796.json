[
    {
        "id": 38578,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "都来消帝力，全不用兵防。"
        ]
    },
    {
        "id": 38579,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "道远擎空钵，深山踏落花。"
        ]
    },
    {
        "id": 38580,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "船中江上景，晚泊早行时。"
        ]
    },
    {
        "id": 38581,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "山寺钟楼月，江城鼓角风。"
        ]
    },
    {
        "id": 38582,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "雨过花争出，云空月半生。"
        ]
    },
    {
        "id": 38583,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "鸟正啼隋柳，人须入楚山。"
        ]
    },
    {
        "id": 38584,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "高松飘雨雪，一室掩香灯。"
        ]
    },
    {
        "id": 38585,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "遂有冥心者，还寻此境来。"
        ]
    },
    {
        "id": 38586,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "又因风雨夜，重到古松门。"
        ]
    },
    {
        "id": 38587,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "道晦金鸡伏，时来木马鸣。"
        ]
    },
    {
        "id": 38588,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "谁来看山寺，自去扫松门。"
        ]
    },
    {
        "id": 38589,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "此生还自喜，余事不相侵。"
        ]
    },
    {
        "id": 38590,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "白鬓无心镊，青山得意多。"
        ]
    },
    {
        "id": 38591,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "一春能几日，无雨亦多风。"
        ]
    },
    {
        "id": 38592,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "苦雨秋涛涨，狂风野烧翻。"
        ]
    },
    {
        "id": 38593,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "须知三尺剑，只为不平磨。"
        ]
    },
    {
        "id": 38594,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "要路争先进，闲门肯暂过。"
        ]
    },
    {
        "id": 38595,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "已难消永夜，况复听秋霖。"
        ]
    },
    {
        "id": 38596,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "大雪路亦宿，深山水也吞。"
        ]
    },
    {
        "id": 38597,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "卷帘黄叶落，锁印子规啼。"
        ]
    },
    {
        "id": 38598,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "亡国空流水，孤祠掩落花。"
        ]
    },
    {
        "id": 38599,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "山魅隔窗舞，𫛳鸟入帘飞。"
        ]
    },
    {
        "id": 38600,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "万里八九月，一身西北风。"
        ]
    },
    {
        "id": 38601,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "磴危攀薜荔，石滑疏莓苔。"
        ]
    },
    {
        "id": 38602,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "日落无行客，天寒有去鸿。"
        ]
    },
    {
        "id": 38603,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "正思浮世事，又到古城边。"
        ]
    },
    {
        "id": 38604,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "寻常风雨夜，应有鬼神看。"
        ]
    },
    {
        "id": 38605,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "寄宿山中寺，相辞海上僧。"
        ]
    },
    {
        "id": 38606,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "风吹榆荚叶，雨打木瓜花。"
        ]
    },
    {
        "id": 38607,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "是星皆拱北，无水不朝东。"
        ]
    },
    {
        "id": 38608,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "西风起边雁，一一向潇湘。"
        ]
    },
    {
        "id": 38609,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "那堪怀远路，犹自上高楼。"
        ]
    },
    {
        "id": 38610,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "白云风散尽，红叶水流来。"
        ]
    },
    {
        "id": 38611,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "妾如无异意，君弃也甘心。"
        ]
    },
    {
        "id": 38612,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "野过云根阔，山高树影长。"
        ]
    },
    {
        "id": 38613,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "古迹应重别，生缘不暂停。"
        ]
    },
    {
        "id": 38614,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "一个字未稳，数宵心不闲。"
        ]
    },
    {
        "id": 38615,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "别地叶频落，去程山已寒。"
        ]
    },
    {
        "id": 38616,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "但取诗名得，何论下第频。"
        ]
    },
    {
        "id": 38617,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "早起赴前程，邻鸡尚未鸣。"
        ]
    },
    {
        "id": 38618,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "白地一回雨，儿孙拾得金。"
        ]
    },
    {
        "id": 38619,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "轩车在何处，雨雪满前山。"
        ]
    },
    {
        "id": 38620,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "灯微山馆雨，角咽海城秋。"
        ]
    },
    {
        "id": 38621,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "去帆看已远，临水立多时。"
        ]
    },
    {
        "id": 38622,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "家贫为客早，路远得书稀。"
        ]
    },
    {
        "id": 38623,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "瀑布五千仞，草堂瀑布边。"
        ]
    },
    {
        "id": 38624,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "我忆云门寺，门前千万峰。"
        ]
    },
    {
        "id": 38625,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "一瓶居无外，万木老禅中。"
        ]
    },
    {
        "id": 38626,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "秋江洗一钵，寒日晒三衣。"
        ]
    },
    {
        "id": 38627,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "空山行客少，独树晚蝉多。"
        ]
    },
    {
        "id": 38628,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "一片月生海，几家人上楼。"
        ]
    },
    {
        "id": 38629,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "窗寒风渐紧，灯落夜将深。"
        ]
    },
    {
        "id": 38630,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "云梦几行去，潇湘一夜空。"
        ]
    },
    {
        "id": 38631,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "无风来竹院，有月在莎庭。"
        ]
    },
    {
        "id": 38632,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "故园从小别，夜雨近秋闻。"
        ]
    },
    {
        "id": 38633,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "书剑别南州，炎荒万里游。"
        ]
    },
    {
        "id": 38634,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "山盘樵径上，人到雪房迟。"
        ]
    },
    {
        "id": 38635,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "路经秋雨后，人入乱峰来。"
        ]
    },
    {
        "id": 38636,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "独树知秋早，孤舟觉夜寒。"
        ]
    },
    {
        "id": 38637,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "马饥餐落叶，鹤宿晒残阳。"
        ]
    },
    {
        "id": 38638,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "路远春行尽，家贫愁到时。"
        ]
    },
    {
        "id": 38639,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "临水通宵坐，知君此兴同。"
        ]
    },
    {
        "id": 38640,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "江声兼小雨，暝色入啼猿。"
        ]
    },
    {
        "id": 38641,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "藕隐玲珑玉，花藏缥缈容。"
        ]
    },
    {
        "id": 38642,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "红树醉秋色，碧溪弹夜弦。"
        ]
    },
    {
        "id": 38643,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "网断蛛犹织，梁空燕不归。"
        ]
    },
    {
        "id": 38644,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "倒身无著处，呵手不成温。"
        ]
    },
    {
        "id": 38645,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "几多诗弟子，无限酒知闻。"
        ]
    },
    {
        "id": 38646,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "床上小熏笼，韶州新退红。"
        ]
    },
    {
        "id": 38647,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "看时人步涩，展处蝶争来。"
        ]
    },
    {
        "id": 38648,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "兢字德源，长安人，梁宰相也。"
        ]
    },
    {
        "id": 38649,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "酒味杂莲气，香冷胜于冰。"
        ]
    },
    {
        "id": 38650,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "太宗皇帝真长策，赚得英雄尽白头。"
        ]
    },
    {
        "id": 38651,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "曾题名处添前字，送出城人乞旧衣。"
        ]
    },
    {
        "id": 38652,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "华阳观里钟声集，建福门前鼓动期。"
        ]
    },
    {
        "id": 38653,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "陇右诸侯供语鸟，日南太守送名花。"
        ]
    },
    {
        "id": 38654,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "八百孤寒齐下泪，一时回首望崖州。"
        ]
    },
    {
        "id": 38655,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "让劫已令多士伏，沽名还得世人闻。"
        ]
    },
    {
        "id": 38656,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "人为子推初禁火，花愁青女再飞霜。"
        ]
    },
    {
        "id": 38657,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "楝花开后风光好，梅子黄时雨意浓。"
        ]
    },
    {
        "id": 38658,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "兰汤备浴传荆俗，水马浮江吊屈魂。"
        ]
    },
    {
        "id": 38659,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "城头椎鼓传花枝，席上抟拳握松子。"
        ]
    },
    {
        "id": 38660,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "鸟向不香花里宿，人从无影月中归。"
        ]
    },
    {
        "id": 38661,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "轻著冻痕销水面，密随和气入花根。"
        ]
    },
    {
        "id": 38662,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "万木尽如花落候，四簷鸣□雨来时。"
        ]
    },
    {
        "id": 38663,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "浓绿万枝红一点，动人春色不须多。"
        ]
    },
    {
        "id": 38664,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "晚菘细切肥牛肚，新笋初尝嫩马蹄。"
        ]
    },
    {
        "id": 38665,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "蝉离楚树鸣犹少，叶到嵩山落更多。"
        ]
    },
    {
        "id": 38666,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "风和日暖方开眼，雨润烟浓不举头。"
        ]
    },
    {
        "id": 38667,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "窗前闲咏鸳鸯句，壁上时观獬廌图。"
        ]
    },
    {
        "id": 38668,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "可能有事关心后，得似无人识面时。"
        ]
    },
    {
        "id": 38669,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "一点孤灯人梦觉，万重寒叶雨声多。"
        ]
    },
    {
        "id": 38670,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "此心只待相逢说，时复登楼看远山。"
        ]
    },
    {
        "id": 38671,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "九江有浪船难济，三峡无猿客有愁。"
        ]
    },
    {
        "id": 38672,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "四海鱼龙精魄冷，五山鸾凤骨毛寒。"
        ]
    },
    {
        "id": 38673,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "谁家绿酒欢留客，何处红楼睡失明。"
        ]
    },
    {
        "id": 38674,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "雨露施恩无厚薄，蓬蒿随分有枯荣。"
        ]
    },
    {
        "id": 38675,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "去年花下留连饮，暖日夭桃莺乱啼。"
        ]
    },
    {
        "id": 38676,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "今日江边容易别，淡烟衰草马频嘶。"
        ]
    },
    {
        "id": 38677,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "蛇蝎性灵生便毒，蕙兰根异死犹香。"
        ]
    },
    {
        "id": 38678,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "三间茅屋无人到，十里松门独自游。"
        ]
    },
    {
        "id": 38679,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "翻忆旧山青碧里，绕庵闲伴野僧禅。"
        ]
    },
    {
        "id": 38680,
        "title": "句",
        "author": "无名氏",
        "volume": "卷七百九十六",
        "paragraphs": [
            "不甘五等诸侯荐，直肯九重天子知。"
        ]
    }
]