[
    {
        "id": 42309,
        "title": "中宗引谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "冬至长于岁。"
        ]
    },
    {
        "id": 42310,
        "title": "贾言忠引谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "军无媒，中道回。"
        ]
    },
    {
        "id": 42311,
        "title": "李绩引谚别张文瓘",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "千里相送，终于一别。"
        ]
    },
    {
        "id": 42312,
        "title": "路励行引谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "一人在朝，百人缓带。"
        ]
    },
    {
        "id": 42313,
        "title": "郝南容引谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "三公后，出死狗。"
        ]
    },
    {
        "id": 42314,
        "title": "员庄谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "上有天堂，下有员庄。"
        ]
    },
    {
        "id": 42315,
        "title": "娄师德引谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "卒客无卒主人。"
        ]
    },
    {
        "id": 42316,
        "title": "台中谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "免巡未推，只得自知。"
        ]
    },
    {
        "id": 42317,
        "title": "宋守敬引谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "双陆无休势。"
        ]
    },
    {
        "id": 42318,
        "title": "雒谷谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "謥洞入黄泉。"
        ]
    },
    {
        "id": 42319,
        "title": "三门谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "古无门匠墓。"
        ]
    },
    {
        "id": 42320,
        "title": "张果引谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "娶妇得公主，平地生公府。"
        ]
    },
    {
        "id": 42321,
        "title": "哥舒翰引谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "狐向窟嗥不祥。"
        ]
    },
    {
        "id": 42322,
        "title": "代宗引谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "不痴不聋，不作阿家阿翁。"
        ]
    },
    {
        "id": 42323,
        "title": "鬼门关谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "鬼门关，十人去，九不还。"
        ]
    },
    {
        "id": 42324,
        "title": "河北谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "山东一条葛，无事莫撩拨。"
        ]
    },
    {
        "id": 42325,
        "title": "李振引谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "百岁奴事三岁主。"
        ]
    },
    {
        "id": 42326,
        "title": "王彦章引谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "人死留名，豹死留皮。"
        ]
    },
    {
        "id": 42327,
        "title": "《孙光宪北梦琐言》引谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "小舅小叔，相追相逐。"
        ]
    },
    {
        "id": 42328,
        "title": "谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "枣子塞鼻孔，悬栖阁却种。",
            "蝉鸣蛁蟟唤，黍种糕糜断。"
        ]
    },
    {
        "id": 42329,
        "title": "宁茵事谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "鹁鸠树上鸣，意在麻子地。"
        ]
    },
    {
        "id": 42330,
        "title": "鸬鹚谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "鸬鹚不打脚下塘。"
        ]
    },
    {
        "id": 42331,
        "title": "盐铁谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "扬一益二。"
        ]
    },
    {
        "id": 42332,
        "title": "冯翊谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "苦泉羊，洛水浆。"
        ]
    },
    {
        "id": 42333,
        "title": "丹徒谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "生东吴，死丹徒。"
        ]
    },
    {
        "id": 42334,
        "title": "湖州里谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "放尔生，放尔命，放尔湖州做百姓。"
        ]
    },
    {
        "id": 42335,
        "title": "益阳谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "长沙益阳，一时相b3。"
        ]
    },
    {
        "id": 42336,
        "title": "昭潭谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "昭潭无底橘州浮。"
        ]
    },
    {
        "id": 42337,
        "title": "江右四郡谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "筠袁赣吉，脑后插笔。"
        ]
    },
    {
        "id": 42338,
        "title": "徐闻谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "欲拔贫，诣徐闻。"
        ]
    },
    {
        "id": 42339,
        "title": "陇西谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "郎驱女驱，十马九驹。安阳大角，十牛九犊。"
        ]
    },
    {
        "id": 42340,
        "title": "荆棺峡谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "九子不葬父，一女打荆棺。"
        ]
    },
    {
        "id": 42341,
        "title": "南中谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "秋收稻，夏收头。"
        ]
    },
    {
        "id": 42342,
        "title": "事狐神谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "无狐魅，不成村。"
        ]
    },
    {
        "id": 42343,
        "title": "李哲家怪引谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "一鸡死，一鸡鸣。"
        ]
    },
    {
        "id": 42344,
        "title": "俗谚",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "白日无谈人，谈人则害生。昏夜无说鬼，说鬼则怪至。"
        ]
    },
    {
        "id": 42345,
        "title": "李白名许云封谜",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "树下彼何人，不语真吾好。语若及日中，烟霏谢成宝。"
        ]
    },
    {
        "id": 42346,
        "title": "许氏碑阴谜",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "谈马砺毕，王田数七。"
        ]
    },
    {
        "id": 42347,
        "title": "大明寺壁语",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "一人堂堂，二曜同光。泉深尺一，点去冰傍。二人相连，"
        ]
    },
    {
        "id": 42348,
        "title": "曹著与客谜",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "一物坐也坐，卧也坐，行也坐。",
            "一物坐也卧，立也卧，行也卧，走也卧，卧也卧"
        ]
    },
    {
        "id": 42349,
        "title": "客题青龙寺门",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "龛龙去东海，时日隐西斜。敬文今不在，碎石入流沙。"
        ]
    },
    {
        "id": 42350,
        "title": "陶谷题南唐官舍壁",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "西川狗，百姓眼。马包儿，御厨饭。"
        ]
    },
    {
        "id": 42351,
        "title": "谢小娥梦盗姓名",
        "author": "不详",
        "volume": "卷八百七十七",
        "paragraphs": [
            "车中猴，门东草。禾中走，一日夫。"
        ]
    }
]